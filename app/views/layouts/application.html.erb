<!DOCTYPE html>
<html>
  <head>
    <title>TailwindPro</title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <script src="http://ajax.googleapis.com/ajax/libs/jquery/2.2.4/jquery.min.js" type="text/javascript"></script>
    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_pack_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_pack_tag 'owl', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_pack_tag 'theme', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'owl', 'data-turbolinks-track': 'reload' %>

  </head>

  <body class='flex flex-col min-h-screen'>
    <%= render 'components/header' %>
    <%= yield %>
    <%= render 'components/footer' %>
  </body>
</html>
